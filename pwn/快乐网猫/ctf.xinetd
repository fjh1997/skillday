service ctf
{
    disable = no
    socket_type = stream
    protocol    = tcp
    wait        = no
    user        = root
    type        = UNLISTED
    port        = 9999
    bind        = 0.0.0.0
    server      = /bin/bash
    # replace helloworld to your program
    server_args = -c "echo flag{h4PpY_neTcat} 2>&1"
    banner_fail = /etc/banner_fail
    per_source	= 5
    rlimit_cpu	= 3
    #rlimit_as  = 1024M # the Address Space resource 
}
